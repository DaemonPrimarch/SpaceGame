config_version=3

[application]

config/name="SpaceGameV2"
run/main_scene="res://nodes/world.tscn"
config/icon="res://icon.png"

[autoload]

MATHS="*res://scripts/singletons/maths.gd"
GUI="*res://scripts/singletons/GUI.gd"
DIALOG_SYSTEM="*res://scripts/singletons/dialog_system.gd"
ROOM_MANAGER="*res://scripts/singletons/room_manager.gd"
PAUSE_MANAGER="*res://scripts/singletons/pause_manager.gd"
PHYSICS_HELPER="*res://scripts/singletons/physics_helper.gd"
CAMERA_MANAGER="*res://scripts/singletons/camera_manager.gd"
SAVE_MANAGER="*res://scripts/singletons/save_manager.gd"

[debug]

settings/fps/force_fps=60
settings/output/verbose=true
settings/debug_labels_enabled=true

[display]

window/size/width=1664

script/source = "tool

extends Node2D

export var enabled = true setget set_enabled

func set_enabled(val):
	enabled = val
	
	if(is_inside_tree()):
		if(enabled):
			$ExtendedAnimationPlayer.play(\"lit\")
		else:
			$ExtendedAnimationPlayer.play(\"darkened\")

func _ready():
	set_enabled(enabled)
	
	if(not Engine.editor_hint):
		get_parent().connect(\"room_entered\", self, \"_on_player_room_entered\")

func _on_player_room_entered():
	if(get_node(\"/root/ROOM_MANAGER\").get_room_of_node(self).is_dark()):
		enabled = true
	else:
		enabled = false"


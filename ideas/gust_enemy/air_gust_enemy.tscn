script/source = "extends \"res://addons/state_handler/state_handler.gd\"

export var air_bast_time = 5

func process_state(delta):
	.process_state(delta)
	if(get_timer() >= air_bast_time):
		get_parent().set_state(\"IDLE\")
	else:
		for node in $air_blasting_area.get_overlapping_bodies():
			if(node.is_in_group(\"player\") and not node.is_grounded()):
				node.move_and_collide(Vector2(0, -64 * 6) * delta)

func enter_state(prev):
	.enter_state(prev)
	$Particles2D.emitting = true
	$ExtendedAnimationPlayer.play(\"air_blast_enter\")

func leave_state(new):
	.leave_state(new)
	$Particles2D.emitting = false
	$ExtendedAnimationPlayer.play(\"air_blast_exit\")"

